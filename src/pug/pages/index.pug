extends ../main



block variables
  - var title = 'Главная'

block content
  section.section.banner
    .container.banner__container
      .banner__content 
        h3.h3title.banner__subtitle Meet bello
        h1.h1title.banner__title A new kind of life insurance
        p.section-descr.banner__descr Secure your family's financial future, in as little as 10 minutes.
        button.primary-btn.banner__btn Get my price
      .banner__img
  section.section.secure
    .container.secure__container 
      .secure__img.secure-img
        img(src="/images/secure_img.jpg", alt="$1 million of coverage")
        .secure-img__descr 
          span.secure-img__star *
          | Data for illustrative purposes
      .secure__content
        h3.h3title.secure__subtitle Secure your future
        h2.h2title.secure__title It’s more affordable than you think
        p.section-descr.secure__descr Life insurance shouldn’t be confusing or expensive. Bello’s term life insurance makes protecting your loved ones easy and affordable.
        a(href="#").primary-link Get My Price
  section.section.technology
    .container.technology__container
      .technology__content
        h3.h3title.technology__subtitle Technology meets service
        h2.h2title.technology__title We're here to help
        p.section-descr.technology__descr Bella, our smart bot, can guide you to your perfect policy. There’s also human help whenever you need it.
        a(href="#").primary-link.technology__link Get help
